{
  "expo": {
    "name": "blocktalk",
    "owner": "asarelcastellanos",
    "slug": "blocktalk",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Chatsnap needs access to your Camera.",
        "NSMicrophoneUsageDescription": "Chatsnap needs access to your Microphone."
      },
      "bundleIdentifier": "com.asarelcastellanos.blocktalk"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "react-native-vision-camera",
        {
          "cameraPermissionText": "Chatsnap needs access to your Camera.",
          "enableMicrophonePermission": true,
          "microphonePermissionText": "Chatsnap needs access to your Microphone."
        }
      ]
    ]
  }
}
